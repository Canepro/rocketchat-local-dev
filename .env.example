# Port where Traefik serves Rocket.Chat on your machine
TRAEFIK_HTTP_PORT=8080

# Images (override if needed)
TRAEFIK_IMAGE=traefik:v2.11
MONGO_IMAGE=mongo:6.0
REDIS_IMAGE=redis:7-alpine
ROCKETCHAT_IMAGE=rocketchat/rocket.chat:7.5.1

# Optional: set a project name to avoid container name collisions
COMPOSE_PROJECT_NAME=rocketchat-local-dev